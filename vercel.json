{
"functions": {
    "api/**/*.py": {
      "excludeFiles": "static/**"
    }
  }
}

{
    "version": 2,
    "builds": [
        {
            "src": "./index.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}


